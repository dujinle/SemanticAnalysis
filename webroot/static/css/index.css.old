heml,
body,
ul,
li {
  margin: 0;
  padding: 0;
  font-family: "Microsoft YaHei", "微软雅黑", "SimHei", "黑体";
}
input,
button {
  outline: none;
  word-wrap: break-word;
}
ul,
li {
  list-style: none;
}
.box {
  width: 100%;
  height: 100%;
  padding: 10px 20px;
  box-sizing: border-box;
}
.box .header {
  width: 100%;
  height: 50px;
  line-height: 50px;
  text-align: center;
  font-size: 30px;
  border: 1px solid #000;
  box-sizing: border-box;
  background: #42abf8;
}
.box .header .__show_text,
.box .header .__show_button {
  width: 80px;
  height: 25px;
  line-height: 25px;
  text-align: center;
  float: right;
  margin: 10px;
  font-size: 20px;
}
.box .header .__show_content {
  width: 500px;
  height: 30px;
  position: absolute;
  right: -20px;
  top: 25px;
  cursor: pointer;
}
.box .header .__show_content li {
  font-size: 16px;
  width: 80px;
  height: 20px;
  line-height: 20px;
  border: 1px solid #000;
  float: left;
  background: white;
}
.box .header .__show_content .select {
  color: white;
  background: #F8A842;
}
.box .main {
  width: 100%;
  height: 100%;
  border-bottom: 1px solid #000;
  overflow: hidden;
}
.box .main .left {
  box-sizing: border-box;
  width: 30%;
  border-right: 1px solid #000 ;
  border-left: 1px solid #000 ;
  height: 618px;
  padding: 10px;
  float: left;
}
.box .main .left .margin {
  margin-bottom: 10px;
}
.box .main .left .text {
  display: inline-block;
  font-size: 20px;
  text-align: center;
  width: 100px;
  height: 20px;
  line-height: 20px;
  vertical-align: top;
  float: left;
}
.box .main .left .one {
  width: 264px;
  height: 20px;
  line-height: 20px;
}
.box .main .left .two_text {
  width: 264px;
  height: 150px;
  border: 1px solid #000;
  margin-left: 100px;
}
.box .main .left .three_text {
  width: 264px;
  height: 350px;
  border: 1px solid #000;
  margin-left: 100px;
}
.box .main .left .button_box {
  width: 30%;
  margin: 0 auto;
}
.box .main .left .button_box .button {
  width: 100px;
  height: 30px;
  line-height: 30px;
  text-align: center;
}
.box .main .right {
  width: auto;
  height: 100%;
  border-right: 1px solid #000;
  margin-left: 30%;
  box-sizing: border-box;
}
.box .main .right .top {
  box-sizing: border-box;
  padding: 10px;
  border-bottom: 1px solid #000;
}
.box .main .right .top .input {
  width: 100px;
  height: 25px;
  vertical-align: top;
}
.box .main .right .top .input_button {
  width: 70px;
  height: 25px;
}
.box .main .right .top .top_button {
  margin-left: 70px;
  margin-top: 10px;
  width: 70px;
  height: 20px;
  line-height: 20px;
}
.box .main .right .top .del {
  height: 70px;
  width: auto;
  float: left;
}
.box .main .right .top .add {
  height: 70px;
  width: 300px;
  margin-left: 320px;
}
.box .main .right .R_left {
  border-right: 1px solid #000000;
  width: 60%;
  height: 100%;
  float: left;
  padding: 0 10px;
  box-sizing: border-box;
}
.box .main .right .R_left .title {
  font-size: 15px;
  font-weight: bold;
}
.box .main .right .R_left .text {
  width: 200px;
  height: 25px;
  line-height: 25px;
}
.box .main .right .R_left .button {
  display: inline-block;
  width: 50px;
  height: 25px;
  line-height: 25px;
  border: 1px solid #000000;
  border-radius: 5px;
  text-align: center;
  cursor: pointer;
}
.box .main .right .R_left .button:active {
  color: white;
  background: #42abf8;
}
.box .main .right .R_left .M,
.box .main .right .R_left .C {
  border-bottom: 1px solid #000;
  height: 50px;
  line-height: 50px;
}
.box .main .right .R_left .C {
  border-bottom: none !important;
}
.box .main .right .R_left .M1,
.box .main .right .R_left .F,
.box .main .right .R_left .F1,
.box .main .right .R_left .X,
.box .main .right .R_left .X1 {
  height: 85px;
  padding: 5px 0;
  box-sizing: border-box;
  border-bottom: 1px solid #000;
}
.box .main .right .R_left .M1 .row_1,
.box .main .right .R_left .F .row_1,
.box .main .right .R_left .F1 .row_1,
.box .main .right .R_left .X .row_1,
.box .main .right .R_left .X1 .row_1 {
  height: 40px;
  line-height: 40px;
}
.box .main .right .R_left .M1 .row_2,
.box .main .right .R_left .F .row_2,
.box .main .right .R_left .F1 .row_2,
.box .main .right .R_left .X .row_2,
.box .main .right .R_left .X1 .row_2 {
  margin-top: 10px;
  height: 20px;
  line-height: 20px;
}
.box .main .right .R_right {
  padding-left: 60%;
  height: 528px;
  box-sizing: content-box;
}
.box .main .right .R_right .r_text {
  padding: 10px;
}

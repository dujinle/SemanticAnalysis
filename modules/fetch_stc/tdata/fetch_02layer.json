{
#词性组合并且发生了词性的变化
#比如 数次和量词组合变为数量词
#动词和助词组合变为形容词
	"MERGE":[
		{
			"start":"SomeNums",
			"end":"SomeUnits",
			"st":"NUM",
			"et":"QUANTIFIER",
			"key":"Nunit",
			"type":"NUNIT",
			"force":"tail"
		},
#情态动词与助词结合变为形容词
		{
			"start":"SomeVerb",
			"end":"SomeAux",
			"st":"PSYCHO",
			"et":"STRU",
			"key":"SomeAdj",
			"type":"ADJ",
			"force":"prev"
		},
#一般动词与助词结合变为动词
		{
			"start":"SomeVerb",
			"end":"SomeAux",
			"st":"ACTION",
			"et":"STRU",
			"key":"SomeVerb",
			"type":"ACTION",
			"force":"all"
		},
#一般动词与助词结合变为名词
		{
			"start":"SomeVerb",
			"end":"SomeAux",
			"st":"ACTION",
			"et":"STRU",
			"key":"SomeVerb",
			"type":"ACTION",
			"force":"all"
		},
#一般动词与感叹助词结合不发生变化
		{
			"start":"SomeVerb",
			"end":"SomeAux",
			"st":"ACTION",
			"et":"MOOD",
			"key":"SomeVerb",
			"force":"prev"
		},
		{
			"start":"SomeVerb",
			"end":"SomeAux",
			"st":"COPULA",
			"et":"STRU",
			"key":"SomeAdj",
			"type":"ADJ",
			"force":"prev"
		},
		{
			"start":"SomePrep",
			"end":"SomeObjs",
			"st":"PREP",
			"et":"SP",
			"key":"SomeVerb",
			"type":"ACTION",
			"force":"all"
		},
#形容词与助词结合
		{
			"start":"SomeAdj",
			"end":"SomeAux",
			"st":"ADJ",
			"et":"AUX",
			"key":"SomeNoun",
			"type":"NOUN",
			"force":"all"
		},
#判定副词修饰一般形容词 变为副词
		{
			"start":"SomeAdverb",
			"end":"SomeAdj",
			"st":"DECIDE",
			"et":"ADJ",
			"key":"SomeAdverb",
			"type":"DECIDE",
			"force":"all"
		}
	],
	"USELESS":[
		"SomeTmood"
	]
}
